{
  "name": "ts-starter",
  "version": "1.0.0",
  "packageManager": "pnpm@6.32.7",
  "description": "typescript-template",
  "keywords": ["typescript", "template"],
  "license": "MIT",
  "author": "peterroe",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "np"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.9",
    "eslint": "^8.11.0",
    "esno": "^0.14.1",
    "np": "^7.6.1",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.0",
    "vite": "^2.8.6",
    "vitest": "^0.7.6"
  }
}