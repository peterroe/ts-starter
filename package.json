{
  "name": "ts-starter",
  "version": "1.0.0",
  "packageManager": "pnpm@7.0.1",
  "description": "typescript-template",
  "keywords": ["typescript", "template"],
  "license": "MIT",
  "author": "peterroe",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "np"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.23.0",
    "eslint": "8.14.0",
    "esno": "0.14.1",
    "np": "7.6.1",
    "typescript": "4.6.4",
    "unbuild": "0.7.4",
    "vite": "2.9.8",
    "vitest": "0.10.5"
  }
}