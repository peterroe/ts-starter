{
  "name": "ts-starter",
  "version": "1.0.0",
  "packageManager": "pnpm@7.9.0",
  "description": "typescript-template",
  "keywords": [
    "typescript",
    "template"
  ],
  "license": "MIT",
  "author": "peterroe",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "release": "bumpp",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.26.1",
    "bumpp": "8.2.1",
    "eslint": "8.21.0",
    "esno": "0.16.3",
    "typescript": "4.7.4",
    "unbuild": "0.8.4",
    "vite": "3.0.7",
    "vitest": "0.21.1"
  }
}